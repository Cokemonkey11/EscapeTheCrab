package CrabWaypoints

import LinkedList

class Waypoint
	vec2 point
	
	construct(vec2 v)
		point = v

LinkedList<Waypoint> waypoints = new LinkedList<Waypoint>()


public function initWayPoints()
	waypoints.add(new Waypoint(vec2(-1000,-3000)))
	waypoints.add(new Waypoint(vec2(3000,4000)))
	waypoints.add(new Waypoint(vec2(-1000,3700)))
	waypoints.add(new Waypoint(vec2(-2000,-4800)))
	waypoints.add(new Waypoint(vec2(4500,-3200)))